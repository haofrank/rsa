{
    "name": "haofrank/rsa",
    "description": "A Laravel 5 package to use RSA in you application",
    "keywords": ["rsa", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hao Frank"
        }
    ],
    "require": {
        "phpseclib/phpseclib": "^2.0",
        "illuminate/console": "^5.6",
        "illuminate/filesystem": "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "HaoFrank\\Rsa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HaoFrank\\Rsa\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
